{
  "name": "dicty-components-header-footer",
  "version": "6.5.0",
  "description": "React component for header and footer of dictyBase web applications",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/"
    ],
    "collectCoverageFrom": [
      "src/components/*.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/es/",
      "!<rootDir>/lib/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "scripts": {
    "build": "nwb build-react-component --no-demo --copy-files",
    "clean": "nwb clean-module && nwb clean-demo",
    "start": "nwb serve-react-demo",
    "test": "jest --updateSnapshot && jest --coverage",
    "test:run": "jest --updateSnapshot && jest",
    "test:watch": "jest --watch",
    "precommit": "lint-staged",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "font-awesome": "^4.7.0",
    "react-fontawesome": "^1.6.1",
    "styled-components": "^2.4.1",
    "typeface-roboto": "0.0.54"
  },
  "peerDependencies": {
    "react": ">= 15.5"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.0.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-react-app": "^3.1.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.13.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.1.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.0.1",
    "lint-staged": "^7.1.3",
    "nwb": "^0.23.0",
    "prettier": "^1.13.4",
    "react": "^16.4.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-styleguidist": "^7.2.3",
    "react-test-renderer": "^16.4.0",
    "style-loader": "^0.18.2"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/dicty-components-header-footer.git"
  },
  "keywords": [
    "react-component"
  ]
}
